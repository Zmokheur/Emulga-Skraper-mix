<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Information ShortName="Emulga - gw"
               LongName="Emulga - Game and Watch"
               Description="Nintendo Game and Watch mix originally by FreemanQC"
               Author="Zmokheur" />
  <Viewport Color="#00FFFFFF" Width="1200" Height="800" />
  <Drawings>
    <Item IfSystem="IsNotInTheListOrAncestors:52" Type="LocalFile" LocalFilePath="NotForYou.png">
      <Display X="50%" Y="50%" Width="100%" Height="100%" Anchor="VCenterHCenter" />
    </Item>
    <Item IfSystem="IsInTheListOrAncestors:52" TestOnly="true">
      <Children>
        <!-- Support Texture -->
         <Item Type ="SupportTexture" >
            <Display X="50%" Y="0%" Width="75%" Height="75%" KeepRatio="true" Antialiasing="High" Anchor="TopHCenter" />
            <Fallback Type="ScreenshotTitle">
                <Fallback Type="LocalFile" LocalFilePath="NoScreenshot.png" />
            </Fallback>
        </Item>
        <!-- Wheel on right -->
        <Item Type="Wheel">
          <Display X="100%" Y="100%" Width="50%" Height="33.3%" Anchor="BottomRight" />
        </Item>
        <!-- 3D Box -->
        <Item Type="Box3D">
          <Display X="0%" Y="100%" Width="50%" Height="75%" Anchor="BottomLeft" />
        </Item>
        <!-- Region -->
        <Item Type="Region1">
           <Display X="100%" Y="0%" Width="10%" Height="10%" KeepRatio="true" Anchor="TopRight" />
        </Item>
        <Item Type="Region2">
           <Display X="100%" Y="12%" Width="10%" Height="10%" KeepRatio="true" Anchor="TopRight" />
        </Item>
      </Children>
    </Item>
  </Drawings>
</ImageComposition>
